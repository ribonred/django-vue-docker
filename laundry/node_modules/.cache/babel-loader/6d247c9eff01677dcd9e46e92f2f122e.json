KAAA,GAAA,MAAA,CAAA,SAAA,CAAA,OAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA;;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,OALA;AAMA,KA1BA;AA2BA,IAAA,IA3BA,gBA2BA,CA3BA,EA2BA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,EAAA,GAAA,CAAA,CAAA,EAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,CAAA,CAAA,IAAA;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,CAAA,CAAA,MAAA;AACA,KAhCA;AAiCA,IAAA,MAjCA,kBAiCA,IAjCA,EAiCA;AAAA;;AACA,aAAA,KAAA,CACA,GADA,CACA,GAAA,GAAA,IAAA,CAAA,EAAA,GAAA,GADA,EACA;AACA,QAAA,IAAA,EAAA,KAAA,IAAA,CAAA,IADA;AAEA,QAAA,MAAA,EAAA,KAAA,IAAA,CAAA;AAFA,OADA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,EAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,IAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,MAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,KAAA;AACA,OAZA,CAAA;AAaA;AA/CA;AAfA,CAAA","sourcesContent":["<template>\n<v-container>\n  <v-row justify=\"center\">\n    <v-col cols=\"12\" sm=\"10\" md=\"8\" lg=\"6\">\n      <v-card ref=\"form\">\n        <form @submit.prevent=\"add\">  \n        <v-card-text>\n          <v-text-field\n            v-model=\"form.nama\"\n            label=\"Nama\"\n            placeholder=\"input nama\"\n            required\n          ></v-text-field>\n          <v-text-field\n            v-model=\"form.alamat\"\n            label=\"Alamat\"\n            placeholder=\"input alamat\"\n            counter=\"25\"\n            required\n          ></v-text-field> \n        </v-card-text>\n        \n        <v-divider class=\"mt-12\"></v-divider>\n        <v-card-actions>\n          <v-btn text>Cancel</v-btn>\n          <v-spacer></v-spacer>\n          <v-btn color=\"primary\" text v-show=\"!updateSubmit\" type=\"submit\">Tambah</v-btn>\n          <v-btn color=\"primary\" text v-show=\"updateSubmit\" type=\"button\"  @click=\"update(form)\">Update</v-btn>\n        </v-card-actions>\n        </form>\n       <v-sheet class=\"text-center\">PELANGGAN</v-sheet>\n    <v-simple-table>\n    <template v-slot:default>\n      <thead>\n        <tr>\n          <th class=\"text-left\">ID</th>\n          <th class=\"text-left\">Nama</th>\n          <th class=\"text-left\">Alamat</th>\n          <th class=\"text-left\">Edit</th>\n          <th class=\"text-left\">Delete</th>\n        </tr>\n      </thead>\n      <tbody>\n        \n        <tr v-for=\"a in pelanggan\" :key=\"a.id\">\n          <td>{{ a.id }}</td>\n          <td>{{ a.nama }}</td>\n          <td>{{ a.alamat }}</td>\n          <td><v-btn color=\"primary\" text @click=\"edit(a)\">Edit</v-btn></td>\n          <td><v-btn color=\"warning\" text @click=\"del(a)\">delete</v-btn></td>\n        </tr>\n\n      </tbody>\n    </template>\n  </v-simple-table>     \n      </v-card>\n    </v-col>\n  </v-row>\n\n</v-container>\n</template>\n\n<script>\nimport axios from \"axios\";\nconst url = \"http://192.168.43.205:8000/pelanggan/\";\nexport default {\n  data() {\n    return {\n      form: {\n        id: \"\",\n        nama: \"\",\n        alamat: \"\"\n      },\n      pelanggan: \"\",\n      updateSubmit: false\n    };\n  },\n  mounted() {\n    this.load();\n  },\n  methods: {\n    load() {\n      axios.get(url).then(res => {\n        this.pelanggan = res.data;\n      });\n    },\n    add() {\n      axios\n        .post(url, {\n          nama: this.form.nama,\n          alamat: this.form.alamat\n        })\n        .then(res => {\n          this.load();\n          this.pelanggan = res.data;\n          this.form.nama = \"\";\n          this.form.alamat = \"\";\n        });\n    },\n    del(a) {\n      axios.delete(url + a.id).then(res => {\n        this.load();\n        let index = this.pelanggan.indexOf(this.form.nama);\n        this.pelanggan.splice(index, 1);\n        this.pelanggan = res.data;\n      });\n    },\n    edit(a) {\n      this.updateSubmit = true;\n      this.form.id = a.id;\n      this.form.nama = a.nama;\n      this.form.alamat = a.alamat;\n    },\n    update(form) {\n      return axios\n        .put(url + form.id + \"/\", {\n          nama: this.form.nama,\n          alamat: this.form.alamat\n        })\n        .then(res => {\n          this.load();\n          this.pelanggan = res.data;\n          this.form.id = \"\";\n          this.form.nama = \"\";\n          this.form.alamat = \"\";\n          this.updateSubmit = false;\n        });\n    }\n  }\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}